#include <iostream>
using namespace std;

int main() {
  int pd=0;
  int d,e,f,g,h;
  cout<<"enter 5 times"<<endl;
  cin>>d>>e>>f>>g>>h;
  int arr[]={d,e,f,g,h};
  int a=arr[0];
  int arrSize=sizeof(arr)/sizeof(arr[0]);
  for(int i=0; i<arrSize; i++){
    if(arr[i]<a){
      a=arr[i];
      pd+=1;
    }
  }
  cout<<pd<<endl;
  return 0;
}
